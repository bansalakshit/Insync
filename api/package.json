  "scripts": {
    "start": "NODE_ENV=production PORT=8080 nodemon --trace-warnings api-build/index.js",
    "server:dev": "nodemon --trace-warnings -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "server:build": "rm -rf api-build && babel src -s -D -d api-build --presets es2015,stage-0",
    "local:server": "yarn server:build && yarn start",
    "server:build:now": "rm -rf api-build && babel src -s -D -d api-build --presets es2015,stage-0",
    "deploy": "yarn server:build:now && now --target production"
  },
